{
  "name": "@goktus/datavault",
  "version": "1.0.0",
  "description": "Minimal AI Data Repository on Irys - Store and manage AI datasets, embeddings, and PyTorch model files on Irys blockchain",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "require": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "datavault": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build && chmod +x dist/cli.js",
    "start": "bun dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest --passWithNoTests",
    "lint": "eslint src/**/*.ts",
    "upload": "ts-node src/cli.ts upload",
    "query": "ts-node src/cli.ts query",
    "fetch": "ts-node src/cli.ts fetch",
    "latest": "ts-node src/cli.ts latest",
    "balance": "ts-node src/cli.ts balance",
    "examples": "ts-node examples/run-all-examples.ts",
    "example:batch": "ts-node examples/batch-upload-demo.ts",
    "example:advanced": "ts-node examples/advanced-programmatic-usage.ts",
    "example:cicd": "ts-node examples/cicd-integration-examples.ts",
    "example:monitoring": "ts-node examples/monitoring-analytics-demo.ts",
    "example:security": "ts-node examples/security-audit-examples.ts",
    "example:test": "ts-node examples/comprehensive-test-suite.ts",
    "demo": "bun run examples",
    "demo:quick": "ts-node examples/usage-examples.ts"
  },
  "keywords": [
    "ai",
    "data",
    "repository",
    "irys",
    "pytorch",
    "embeddings",
    "blockchain",
    "machine-learning",
    "dataset",
    "model-storage",
    "decentralized-storage",
    "arweave",
    "typescript",
    "cli"
  ],
  "author": "DataVault Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AzrielTheHellrazor/DataVault.git"
  },
  "homepage": "https://github.com/AzrielTheHellrazor/DataVault#readme",
  "bugs": {
    "url": "https://github.com/AzrielTheHellrazor/DataVault/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@irys/sdk": "^0.2.11",
    "sqlite3": "^5.1.6",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "fs-extra": "^11.1.1",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/fs-extra": "^11.0.4",
    "@types/mime-types": "^2.1.4",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1"
  }
}
